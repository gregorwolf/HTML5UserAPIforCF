{
  "welcomeFile": "/comsapsapmentorshtml5userapiforcfui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout",
    "logoutPage": "/comsapsapmentorshtml5userapiforcfui/logout-page.html"
  },
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "/api/(.*)$",
      "target": "/api/$1",
      "authenticationType": "xsuaa",
      "destination": "html5userapi-srv",
      "csrfProtection": true
    },
    {
      "source": "/rfc(.*)$",
      "target": "/rfc$1",
      "authenticationType": "xsuaa",
      "destination": "html5userapi-srv-java",
      "csrfProtection": true
    },
    {
      "source": "/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "SAP_ABAP_BACKEND"
    },
    {
      "source": "/sap_basic/(.*)$",
      "target": "/sap/$1",
      "destination": "SAP_ABAP_BACKEND_BASIC_AUTH"
    },
    {
      "source": "/b1s/(.*)$",
      "target": "/b1s/$1",
      "destination": "SAP_B1_BACKEND"
    },
    {
      "source": "/v2/admin/(.*)$",
      "target": "/v2/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    },
    {
      "source": "/v2/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    },
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    },
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    }
  ]
}