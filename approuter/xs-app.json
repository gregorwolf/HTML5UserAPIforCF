{
  "welcomeFile": "/comsapsapmentorshtml5userapiforcfui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout",
    "logoutPage": "/comsapsapmentorshtml5userapiforcfui/logged-out.html"
  },
  "routes": [
    {
      "source": "/api/(.*)$",
      "target": "/api/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "SAP_ABAP_BACKEND"
    },
    {
      "source": "/b1s/(.*)$",
      "target": "/b1s/$1",
      "destination": "SAP_B1_BACKEND"
    },
    {
      "source": "/v2/admin/(.*)$",
      "target": "/v2/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    },
    {
      "source": "/v2/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    },
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    },
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "bookshop",
      "csrfProtection": true
    }
  ]
}